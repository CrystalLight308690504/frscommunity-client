<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:dist="http://schemas.android.com/apk/distribution"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.crystallightghot.frscommunityclient">

    <dist:module dist:instant="true"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/icon_application"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/icon_application_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <receiver
                android:name=".activity.broadcast.HomeViewPagerItemScrollChangedReceiver"
                android:enabled="true"
                android:exported="true">
            <intent-filter>
                <action android:name="HomeViewPagerItemScrollChangedReceiver"/>
            </intent-filter>
        </receiver>

        <activity android:name=".activity.BaseActivity">
        </activity>
        <activity android:name=".activity.AllSkatingCategoryActivity">
        </activity>
        <activity
                android:name=".activity.HomeActivity"
                android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".AccessActivity">
        </activity>
    </application>

</manifest>